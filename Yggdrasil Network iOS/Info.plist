	<array>
		<dict>
			<key>UTTypeConformsTo</key>
			<array>
				<string>public.text</string>
			</array>
			<key>UTTypeDescription</key>
			<string>Yggdrasil Configuration File</string>
			<key>UTTypeIconFiles</key>
			<array/>
