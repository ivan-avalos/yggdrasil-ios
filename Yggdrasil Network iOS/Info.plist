			<string>com.dulcedosystems.yggdrasil.configuration</string>
			<key>UTTypeTagSpecification</key>
			<dict>
				<key>public.filename-extension</key>
				<array>
					<string>yggconf</string>
				</array>
			</dict>
		</dict>
	</array>
